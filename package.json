    "vitest": "^4.0.18",
    "wait-on": "^9.0.3"
  },
  "build": {
    "appId": "com.stella.app",
    "productName": "Stella",
    "protocols": {
      "name": "Stella Auth",
      "schemes": ["Stella"]
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "native/mouse_block.exe"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
